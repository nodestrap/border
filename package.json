{
  "name": "@nodestrap/border",
  "version": "2.0.0",
  "description": "A utility for styling the border and border radius of an HTML element.",
  "dependencies": {
    "@nodestrap/colors": "^2.0.1",
    "@nodestrap/sass-class-template": "^2.0.2"
  },
  "devDependencies": {
    "tilde-sass": "^1.0.0"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/",
    "compile": "tilde-sass --file src/*.scss --outFile dist/ --outputStyle compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/border.git"
  },
  "keywords": [
    "border",
    "radius",
    "html",
    "element",
    "css",
    "style"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/border/issues"
  },
  "homepage": "https://github.com/nodestrap/border#readme"
}
